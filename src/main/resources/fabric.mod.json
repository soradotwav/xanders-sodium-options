{
  "schemaVersion": 1,
  "id": "xanders-sodium-options",
  "version": "${mod_version}",
  "name": "Xander's Sodium Options: Continued",
  "description": "Replaces the Sodium options GUI with vanilla-style alternatives.",
  "authors": [
    "isXander",
    "IMB11",
    "soradotwav"
  ],
  "contact": {
    "issues": "https://github.com/soradotwav/xanders-sodium-options/issues",
    "sources": "https://github.com/soradotwav/xanders-sodium-options"
  },
  "icon": "icon.png",
  "license": "LGPL-3.0-or-later",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.isxander.xso.ModEntrypoint"
    ],
    "modmenu": [
      "dev.isxander.xso.config.ModMenuEntrypoint"
    ]
  },
  "mixins": [
    "xanders-sodium-options.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.9",
    "minecraft": "${target_minecraft}",
    "sodium": ">=${target_sodium}",
    "yet_another_config_lib_v3": ">=3.6.0"
  },
  "breaks": {
    "reeses-sodium-options": "*",
    "sodium": "<${target_sodium}"
  }
}
